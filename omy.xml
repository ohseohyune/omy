<mujoco model="open_manipulator_y">
  <compiler angle="radian" meshdir="assets" autolimits="true"/>

  <option integrator="implicitfast" timestep="0.002" gravity="0 0 0"/>

  <!-- Default -->
  <default>
    <default class="Y">
      <joint range="-6.28319 6.28319" armature="0.1" damping="15.0"/>
       <geom contype="1" conaffinity="1"/>
      <position kp="1500" forcerange="-2000 2000" ctrlrange="-6.28319 6.28319"/>
    </default>
    <default class="gripper">
      <joint armature="0.1" damping="10.0"/>
       <geom contype="1" conaffinity="1"/>
      <position kp="1800" forcerange="-1000 1000"/>
    </default>
  </default>

  <asset>
    <mesh name="base_unit" file="base_unit.stl" scale="0.001 0.001 0.001"/>
    <mesh name="link1" file="link1.stl" scale="0.001 0.001 0.001"/>
    <mesh name="link2" file="link2.stl" scale="0.001 0.001 0.001"/>
    <mesh name="link3" file="link3.stl" scale="0.001 0.001 0.001"/>
    <mesh name="link4" file="link4.stl" scale="0.001 0.001 0.001"/>
    <mesh name="link5" file="link5.stl" scale="0.001 0.001 0.001"/>
    <mesh name="link6" file="link6.stl" scale="0.001 0.001 0.001"/>
    <mesh name="flange" file="flange.stl" scale="0.001 0.001 0.001"/>
    <mesh name="base" file="base.stl" scale="0.001 0.001 0.001"/>
    <mesh name="r1" file="r1.stl" scale="0.001 0.001 0.001"/>
    <mesh name="r2" file="r2.stl" scale="0.001 0.001 0.001"/>
    <mesh name="l1" file="l1.stl" scale="0.001 0.001 0.001"/>
    <mesh name="l2" file="l2.stl" scale="0.001 0.001 0.001"/>
  </asset>

  <worldbody>
    <geom type="mesh" rgba="0.2 0.2 0.2 1" mesh="base_unit"/>
    <body name="link1" pos="0 0 0.1715">
      <inertial pos="-0.000110636 -0.00547113 -0.0158977" quat="0.98558 0.168758 -0.00294757 0.0120358" mass="2.06488" diaginertia="0.00303337 0.00279081 0.00221937"/>
      <joint name="joint1" pos="0 0 0" axis="0 0 1" class="Y"/>
      <geom type="mesh" rgba="0.2 0.2 0.2 1" mesh="link1"/>
      <body name="link2" pos="0 -0.1215 0">
        <inertial pos="1.14054e-05 0.0161842 0.103606" quat="0.999988 -0.00477885 -0.000609836 0.00067321" mass="3.67954" diaginertia="0.051732 0.0503576 0.00449718"/>
        <joint name="joint2" pos="0 0 0" axis="0 1 0" class="Y"/>
        <geom type="mesh" rgba="0.2 0.2 0.2 1" mesh="link2"/>
        <body name="link3" pos="0 0 0.247">
          <inertial pos="7.81487e-05 0.107185 0.141173" quat="0.999987 0.00500823 0.000469041 -0.00072733" mass="2.38659" diaginertia="0.0226944 0.021922 0.00277422"/>
          <joint name="joint3" pos="0 0 0" axis="0 1 0" class="Y"/>
          <geom type="mesh" rgba="0.2 0.2 0.2 1" mesh="link3"/>
          <body name="link4" pos="0 0.1215 0.2195">
            <inertial pos="-0.000133183 -0.107467 0.0190907" quat="0.983636 0.179196 0.00338061 -0.0183488" mass="1.40023" diaginertia="0.00175475 0.00162284 0.00115817"/>
            <joint name="joint4" pos="0 0 0" axis="0 1 0" class="Y"/>
            <geom type="mesh" rgba="0.2 0.2 0.2 1" mesh="link4"/>
            <body name="link5" pos="0 -0.113 0">
              <inertial pos="0.000133183 -0.0190907 0.109967" quat="0.822247 0.568825 0.0105841 -0.015365" mass="1.40023" diaginertia="0.00175475 0.00162284 0.00115817"/>
              <joint name="joint5" pos="0 0 0" axis="0 0 1" class="Y"/>
              <geom type="mesh" rgba="0.2 0.2 0.2 1" mesh="link5"/>
              <body name="link6" pos="0 0 0.1155">
                <inertial pos="1.6005e-05 -0.103085 0.00570277" quat="0.456875 0.889528 0.00170634 -0.00130773" mass="0.652774" diaginertia="0.00112656 0.000847594 0.000551829"/>
                <joint name="joint6" pos="0 0 0" axis="0 1 0" class="Y"/>
                <geom type="mesh" rgba="0.2 0.2 0.2 1" mesh="link6"/>
                <geom pos="0 -0.103 0" quat="1 0 0 0" type="mesh" rgba="0.2 0.2 0.2 1" mesh="flange"/>
                <geom pos="0 -0.109 0" quat="0.500398 0.5 0.5 -0.499602" type="mesh" mesh="base"/>
                <body name="cleaning_pad" pos="0 -0.235 -0.018" euler="-0.73 0 0">
                  <inertial pos="0 0 0" mass="0.1" diaginertia="1e-5 1e-5 1e-5"/>
                  <geom  type="box" solref="0.02 1" solimp="0.9 0.99 0.01" size="0.04 0.04 0.02" rgba="0.9 0.9 0.9 1" friction="0.5 0.4 0.01" contype="1" conaffinity="1"/>
                  <site name="tcp_link" pos="0.0 0.0 0.0" size="0.005" rgba="1 0 0 1"/>
                </body>            
                <body name="rh_p12_rn_r1" pos="0.00803822 -0.156994 6.40105e-14" quat="0.500398 0.5 0.5 -0.499602">
                  <inertial pos="0 0.034 0.004" mass="0.068" diaginertia="1e-06 1e-06 1e-06"/>
                  <joint name="rh_r1" pos="0 0 0" axis="1 0 0" range="0 1.1" class="gripper"/>
                  <geom type="mesh" mesh="r1"/>
                  <body name="rh_p12_rn_r2" pos="0 0.0493634 0.0285">
                    <inertial pos="0 0.006 0.011" mass="0.022" diaginertia="1e-06 1e-06 1e-06"/>
                    <joint name="rh_r2" pos="0 0 0" axis="-1 0 0" range="0 1" class="gripper"/>
                    <geom type="mesh" mesh="r2"/>
                  </body>
                </body>
                <body name="rh_p12_rn_l1" pos="-0.00796177 -0.157006 -6.34017e-06" quat="0.500398 0.5 0.5 -0.499602">
                  <inertial pos="0 -0.034 0.004" mass="0.068" diaginertia="1e-06 1e-06 1e-06"/>
                  <joint name="rh_l1" pos="0 0 0" axis="-1 0 0" range="0 1.1" class="gripper"/>
                  <geom type="mesh" mesh="l1"/>
                  <body name="rh_p12_rn_l2" pos="0 -0.0493634 0.0285">
                    <inertial pos="0 -0.006 0.011" mass="0.022" diaginertia="1e-06 1e-06 1e-06"/>
                    <joint name="rh_l2" pos="0 0 0" axis="1 0 0" range="0 1" class="gripper"/>
                    <geom type="mesh" mesh="l2"/>

                  </body>
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
    </body>
  </worldbody>

  <!-- Exclude contact information -->
  <contact>
    <exclude body1="world" body2="link1"/>
  </contact>

  <!-- Define actuators -->
  <actuator>
     <!--
    <position name="actuator_joint1" joint="joint1" class="Y"/>
    <position name="actuator_joint2" joint="joint2" class="Y"/>
    <position name="actuator_joint3" joint="joint3" class="Y"/>
    <position name="actuator_joint4" joint="joint4" class="Y"/>
    <position name="actuator_joint5" joint="joint5" class="Y"/>
    <position name="actuator_joint6" joint="joint6" class="Y"/>
    <position name="actuator_rh_r1" joint="rh_r1" ctrlrange="0.000 1.100" class="gripper"/>
    <position name="actuator_rh_r2" joint="rh_r2" ctrlrange="0.000 1.000" class="gripper"/>
    <position name="actuator_rh_l1" joint="rh_l1" ctrlrange="0.000 1.100" class="gripper"/>
    <position name="actuator_rh_l2" joint="rh_l2" ctrlrange="0.000 1.000" class="gripper"/>
   -->
      
    <!--
    <position name="actuator_rh_r1" joint="rh_r1" ctrlrange="0.000 1.100" class="gripper"/>
    <position name="actuator_rh_r2" joint="rh_r2" ctrlrange="0.000 1.000" class="gripper"/>
    <position name="actuator_rh_l1" joint="rh_l1" ctrlrange="0.000 1.100" class="gripper"/>
    <position name="actuator_rh_l2" joint="rh_l2" ctrlrange="0.000 1.000" class="gripper"/>
    -->
    
    <velocity name="actuator_joint1" joint="joint1" gear="1" forcerange="-500 500"/>
    <velocity name="actuator_joint2" joint="joint2" gear="1" forcerange="-500 500"/>
    <velocity name="actuator_joint3" joint="joint3" gear="1" forcerange="-500 500"/>
    <velocity name="actuator_joint4" joint="joint4" gear="1" forcerange="-500 500"/>
    <velocity name="actuator_joint5" joint="joint5" gear="1" forcerange="-500 500"/>
    <velocity name="actuator_joint6" joint="joint6" gear="1" forcerange="-500 500"/>
  
    <velocity name="actuator_rh_r1" joint="rh_r1" gear="1" forcerange="-500 500" class="gripper"/>
    <velocity name="actuator_rh_r2" joint="rh_r2" gear="1" forcerange="-500 500" class="gripper"/>
    <velocity name="actuator_rh_l1" joint="rh_l1" gear="1" forcerange="-500 500" class="gripper"/>
    <velocity name="actuator_rh_l2" joint="rh_l2" gear="1" forcerange="-00 500" class="gripper"/> 
 <!-- 7~10번 손가락: general actuator (토크 제어) 
  <general  name="actuator_rh_r1" joint="rh_r1" gear="1" forcerange="-500 500"/>
  <general  name="actuator_rh_r2" joint="rh_r2" gear="1" forcerange="-500 500"/>
  <general  name="actuator_rh_l1" joint="rh_l1" gear="1" forcerange="-500 500"/>
  <general  name="actuator_rh_l2" joint="rh_l2" gear="1" forcerange="-500 500"/>
  -->  
  </actuator>


  <sensor>
    <force site="tcp_link"/>
    <torque site="tcp_link"/>
  </sensor>
</mujoco>
